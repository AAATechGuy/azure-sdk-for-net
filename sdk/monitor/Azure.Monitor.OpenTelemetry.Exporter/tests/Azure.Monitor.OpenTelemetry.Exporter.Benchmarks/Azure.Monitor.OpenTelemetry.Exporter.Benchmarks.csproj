<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>

    <!--Mitigate NU1505	Duplicate PackageDownload items found: Microsoft.NETCore.App.Host.win-x64  https://github.com/dotnet/sdk/issues/24747#issuecomment-1126298533-->
    <DisableCheckingDuplicateNuGetItems>true</DisableCheckingDuplicateNuGetItems>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\Azure.Monitor.OpenTelemetry.Exporter.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="BenchmarkDotNet" Version="$(BenchmarkDotNetPkgVer)" />
  </ItemGroup>

</Project>
